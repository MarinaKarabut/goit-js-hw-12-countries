{{#each this}}
<div class="wrapper">
    <h2 class="card-title-country">{{name}}</h2>
    <div class="card">
        <div class="card-body">
            <p class="card-text"><span class="card-text-name">Capital:</span>{{capital}}</p>
            <p class="card-text"><span class="card-text-name">Population:</span>{{population}}</p>
            <p class="card-text"><span class="card-text-name">Languages:</span></p>
            <ul class="list-languages">{{#each languages}}
                <li class="list-languages-item">{{name}}</li>
                {{/each}}
            </ul>
        </div>
        <div class="card-img">
            <img class="card-img-flag" src="{{flag}}" alt="{{name}}" />
        </div>
    </div>
</div>

{{/each}}